# Source: envoy-ingress/templates/all-in-one.yaml
apiVersion: v1
kind: ConfigMap
metadata:
  name: zwierzeta-html
  namespace: envoy-ingress
data:
  landing.html: |
    <h1>Witamy na stronie glownej :)</h1>
