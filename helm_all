          args: ["cp /mnt/chomik.html /usr/share/nginx/html/index.html && nginx -g 'daemon off;'"]
          volumeMounts:
            - name: html
              mountPath: /mnt
              readOnly: true
      volumes:
        - name: html
          configMap:
            name: zwierzeta-html
            items:
              - key: chomik.html
                path: chomik.html
---
# Source: envoy-ingress/templates/all-in-one.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: leopard-html
  namespace: envoy-ingress
spec:
  replicas: 1
  selector:
    matchLabels:
      app: leopard-html
  template:
    metadata:
      labels:
        app: leopard-html
        role: zwierzak-html
    spec:
      containers:
        - name: nginx
          image: nginx:1.25
          command: ["/bin/sh", "-c"]
          args: ["cp /mnt/leopard.html /usr/share/nginx/html/index.html && nginx -g 'daemon off;'"]
          volumeMounts:
            - name: html
              mountPath: /mnt
              readOnly: true
      volumes:
        - name: html
          configMap:
            name: zwierzeta-html
            items:
              - key: leopard.html
                path: leopard.html
---
