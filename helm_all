# Source: envoy-ingress/templates/all-in-one.yaml
apiVersion: v1
kind: ConfigMap
metadata:
  name: envoy-bootstrap
  namespace: envoy-ingress
data:
  envoy.yaml: |
    static_resources:
      listeners:
      - name: listener_http
        address:
          socket_address:
            address: 0.0.0.0
            port_value: 10080
        filter_chains:
        - filters:
          - name: envoy.filters.network.http_connection_manager
            typed_config:
              "@type": type.googleapis.com/envoy.extensions.filters.network.http_connection_manager.v3.HttpConnectionManager
              stat_prefix: ingress_http
              route_config:
                name: local_route
                virtual_hosts:
                - name: landing
                  domains: ["envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: landing_html
                - name: pies
                  domains: ["pies.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: pies_html
                - name: kot
                  domains: ["kot.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: kot_html
                - name: zolw
                  domains: ["zolw.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: zolw_html
                - name: lew
                  domains: ["lew.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: lew_html
                - name: kaczka
                  domains: ["kaczka.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: kaczka_html
                - name: lis
                  domains: ["lis.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: lis_html
                - name: lama
                  domains: ["lama.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: lama_html
                - name: chomik
                  domains: ["chomik.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: chomik_html
                - name: leopard
                  domains: ["leopard.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: leopard_html
                - name: jez
                  domains: ["jez.envoy_zwierzaki.local"]
                  routes:
                  - match:
                      prefix: "/"
                    route:
                      cluster: jez_html
              http_filters:
              - name: envoy.filters.http.router
                typed_config:
                  "@type": type.googleapis.com/envoy.extensions.filters.http.router.v3.Router
      clusters:
      - name: landing_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: landing_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: landing-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: pies_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: pies_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: pies-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: kot_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: kot_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: kot-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: zolw_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: zolw_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: zolw-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: lew_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: lew_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: lew-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: kaczka_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: kaczka_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: kaczka-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: lis_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: lis_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: lis-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: lama_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: lama_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: lama-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: chomik_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: chomik_html
          endpoints:
          - lb_endpoints:
            - endpoint:
                address:
                  socket_address:
                    address: chomik-html.envoy-ingress.svc.cluster.local
                    port_value: 80
      - name: leopard_html
        type: LOGICAL_DNS
        connect_timeout: 0.25s
        lb_policy: ROUND_ROBIN
        load_assignment:
          cluster_name: leopard_html
          endpoints:
