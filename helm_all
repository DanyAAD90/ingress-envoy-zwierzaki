# Source: envoy-ingress/templates/all-in-one.yaml
apiVersion: v1
kind: Service
metadata:
  name: envoy-ingress
  namespace: envoy-ingress
spec:
  type: LoadBalancer
  loadBalancerIP: 192.168.18.215
  selector:
    app: envoy
  ports:
  - port: 80
    targetPort: 10080
---
# Source: envoy-ingress/templates/all-in-one.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: landing-html
  namespace: envoy-ingress
spec:
  replicas: 1
  selector:
    matchLabels:
      app: landing-html
  template:
    metadata:
      labels:
        app: landing-html
    spec:
      containers:
        - name: nginx
          image: nginx:1.25
          command: ["/bin/sh", "-c"]
          args: ["cp /mnt/landing.html /usr/share/nginx/html/index.html && nginx -g 'daemon off;'"]
          volumeMounts:
            - name: html
              mountPath: /mnt
              readOnly: true
      volumes:
        - name: html
          configMap:
            name: zwierzeta-html
            items:
              - key: landing.html
                path: landing.html
---
# Source: envoy-ingress/templates/all-in-one.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: pies-html
  namespace: envoy-ingress
spec:
  replicas: 1
  selector:
    matchLabels:
      app: pies-html
  template:
    metadata:
      labels:
        app: pies-html
        role: zwierzak-html
    spec:
      containers:
        - name: nginx
          image: nginx:1.25
          command: ["/bin/sh", "-c"]
          args: ["cp /mnt/pies.html /usr/share/nginx/html/index.html && nginx -g 'daemon off;'"]
          volumeMounts:
            - name: html
              mountPath: /mnt
              readOnly: true
      volumes:
        - name: html
          configMap:
            name: zwierzeta-html
            items:
              - key: pies.html
                path: pies.html
