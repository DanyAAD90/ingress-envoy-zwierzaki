# Source: envoy-ingress/templates/all-in-one.yaml
apiVersion: apps/v1
kind: Deployment
metadata:
  name: envoy-ingress
  namespace: envoy-ingress
spec:
  revisionHistoryLimit: 0
  replicas: 1
  selector:
    matchLabels:
      app: envoy
  template:
    metadata:
      labels:
        app: envoy
    spec:
      containers:
      - name: envoy
        image: envoyproxy/envoy:v1.30.1
        volumeMounts:
        - name: config
          mountPath: /etc/envoy
        ports:
        - containerPort: 80
      volumes:
      - name: config
        configMap:
          name: envoy-bootstrap
